{
  "expo": {
    "name": "gsop",
    "slug": "gsop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/sgn-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ea5b0c"
    },
    "scheme": "gsop",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "androidStatusBarColor": "#ea5b0c",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ea5b0c"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ea5b0c"
    },
    "android": {
      "package": "uk.co.sgn.gsopcheck"
    }
  }
}
